<launch>

<arg name="cloud_topic" default="/points_raw"/>

  <node name="ground_segmentation" pkg="patchworkpp" type="demo" output="screen">
    <rosparam command="load" file="$(find patchworkpp)/config/my_params.yaml" />
    <param name="cloud_topic" value="$(arg cloud_topic)"/>
  </node>

  <node name="$(anon rviz)" pkg="rviz" type="rviz" args="-d $(find patchworkpp)/rviz/my_patchworkpp.rviz"/>

</launch>
