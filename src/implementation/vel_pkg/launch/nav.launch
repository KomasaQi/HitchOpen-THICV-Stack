<launch>
    <!-- 模型发布节点 -->
   <include file="$(find truck_model)/launch/truck_model.launch"/>

    <!--- Run move_base -->
    <node pkg="move_base" type="move_base" name="move_base">

        <rosparam file="$(find wpb_home_tutorials)/nav_lidar/costmap_common_params.yaml" command="load" ns="global_costmap" />

        <rosparam file="$(find wpb_home_tutorials)/nav_lidar/costmap_common_params.yaml" command="load" ns="local_costmap" />

        <rosparam file="$(find wpb_home_tutorials)/nav_lidar/global_costmap_params.yaml" command="load" />

        <rosparam file="$(find wpb_home_tutorials)/nav_lidar/local_costmap_params.yaml" command="load" />
        <param name="base_global_planner" value="global_planner/GlobalPlanner" /> 
        <param name="base_local_planner" value="my_planner/MyPlanner" />
    
    </node>

    <!-- Run map server -->
    <node pkg="map_server" type="map_server" name="map_server" args="$(find wpr_simulation)/maps/gongwei.yaml"/>

    <!--- Run AMCL -->
    <node pkg="amcl" type="amcl" name="amcl" >
        <param name="odom_model_type" value="omni"/>
    </node>
    
    <!-- Run lidar_loc -->
    <!-- <node pkg="jie_ware" type="lidar_loc" name="lidar_loc" >
        <param name="base_frame" value="base_footprint" />
        <param name="odom_frame" value="odom" />
        <param name="laser_frame" value="laser" />
        <param name="laser_topic" value="scan" />
    </node> -->


    <!--- Run rviz -->
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find wpr_simulation)/rviz/nav.rviz"/>
</launch>
